<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="no.hiof.hiofcommuting.register.EmailLoginActivity"
    tools:ignore="MergeRootFrame" >
    
    <fragment android:name="no.hiof.hiofcommuting.register.LoginFragment" 
          android:id="@+id/loginFragment"
          android:layout_width="match_parent"
          android:layout_height="match_parent" />

      <fragment android:name="no.hiof.hiofcommuting.register.RegisterFragment" 
          android:id="@+id/registerFragment"
          android:layout_width="match_parent"
          android:layout_height="match_parent" />
      
      <fragment android:name="no.hiof.hiofcommuting.register.ForgotPwFragment" 
          android:id="@+id/forgotPwFragment"
          android:layout_width="match_parent"
          android:layout_height="match_parent" />
      
      <fragment android:name="no.hiof.hiofcommuting.register.FinishProfileFragment" 
          android:id="@+id/finishProfileFragment"
          android:layout_width="match_parent"
          android:layout_height="match_parent" />

</FrameLayout>

